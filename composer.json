{
    "name": "artisanry/basket",
    "description": "Powerful Basket for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Basket",
        "Laravel Basket"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "artisanry/service-provider": "^1.1",
        "faustbrian/basket": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Laravel\\Basket\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Laravel\\Basket\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Basket\\BasketServiceProvider"
            ],
            "aliases": {
                "Basket": "Artisanry\\Basket\\Facades\\Basket"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
